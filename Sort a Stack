#include <bits/stdc++.h> 
void insert(stack<int>& s, int temp){
	if(s.size()==0 || s.top()< temp){
		s.push(temp);
		return;
	}
	int x= s.top();
	s.pop();
	insert(s,temp);
	s.push(x);
	return ;
	
}
void sortStack(stack<int> &stack)
{
	if(stack.size()<=1)return ;
	int temp= stack.top();
	stack.pop();
	sortStack(stack);
	insert(stack, temp);
	// Write your code here
}
